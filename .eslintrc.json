{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "react/jsx-filename-extension": 0,
    "default-param-last": 0,
    "no-case-declarations": 0,
    "react/react-in-jsx-scope": 0,
    "react/function-component-definition": 0,
    "max-len": 0,
    "import/prefer-default-export": 2
  }
}